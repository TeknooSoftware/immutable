    "config": {
        "audit": {
            "abandoned": "report"
        },
        "optimize-autoloader": true
    },
    "require":{
        "php": "^8.2"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "^11",
        "squizlabs/php_codesniffer": "^3.8",
        "phpstan/phpstan": "^1.10.57",
        "symfony/property-access": "^6.3||^7.0"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Teknoo\\Immutable\\": ["src/"],
            "Teknoo\\Tests\\Immutable\\": ["tests/"]
        }
    },
    "archive": {
        "exclude": ["docs", "tests" ,"vendor", ".idea"]
    }
}
