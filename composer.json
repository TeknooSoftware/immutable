{
    "name": "teknoo/immutable",
    "type": "library",
    "description": "Library to create PHP immutable object",
    "keywords": ["immutable", "object"],
    "homepage": "http://teknoo.software/immutable",
    "license": ["MIT"],
    "authors": [
        {
            "name": "Richard DÃ©loge",
            "email": "richarddeloge@gmail.com",
            "role":"Software architect, Lead developer"
        }
    ],
    "support": {
        "issues": "https://github.com/TeknooSoftware/immutable/issues",
        "source": "https://github.com/TeknooSoftware/immutable"
    },
    "config": {
        "optimize-autoloader": true
    },
    "require":{
        "php": "~7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "squizlabs/php_codesniffer": "~3.5",
        "pdepend/pdepend": "~2.7",
        "sebastian/phpcpd": "~5.0",
        "phploc/phploc": "~6.0",
        "phpstan/phpstan": "~0.12"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Teknoo\\Immutable\\": ["src/"],
            "Teknoo\\Tests\\Immutable\\": ["tests/"]
        }
    },
    "archive": {
        "exclude": ["docs", "tests" ,"vendor", ".idea"]
    }
}
